{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://defence-intelligence.onrender.com/api/$1"
        },
        {
            "source": "/",
            "destination": "/frontend/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}